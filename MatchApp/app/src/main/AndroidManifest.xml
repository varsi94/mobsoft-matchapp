<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobsoft.matchapp.matchapp">

    <application
        android:name="com.mobsoft.matchapp.MobSoftApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mobsoft.matchapp.ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mobsoft.matchapp.ui.standings.StandingsActivity" />
        <activity android:name="com.mobsoft.matchapp.ui.details.DetailsActivity" />
        <activity android:name="com.mobsoft.matchapp.ui.editor.EditorActivity" />
        <activity android:name="com.mobsoft.matchapp.ui.matchlist.MatchListActivity"></activity>
    </application>

</manifest>